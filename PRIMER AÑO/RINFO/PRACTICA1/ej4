programa p1ej4
procesos
  proceso juntarPapeles
  comenzar
    mientras(HayPapelEnLaEsquina)
      tomarPapel
  fin
  proceso juntarFlores
  comenzar
    mientras(HayFlorEnLaEsquina)
      tomarFlor
  fin
  proceso recorrerPapeles
  comenzar
    repetir 24
      repetir 99
        juntarPapeles
        mover
      juntarPapeles
      Pos(PosAv+1,1)
    repetir 99
      juntarPapeles
      mover
    juntarPapeles
  fin
  proceso recorrerFlores
  comenzar
    repetir 24
      repetir 99
        juntarFlores
        mover
      juntarFlores
      Pos(PosAv+1,1)
    repetir 99
      juntarFlores
      mover
    juntarFlores
  fin
areas
  area1:AreaP(1,1,25,100)
  area2:AreaP(26,1,50,100)
  area3:AreaP(51,1,75,100)
  area4:AreaP(76,1,100,100)
robots
  robot robot1
  comenzar
    recorrerFlores
    Pos(51,1)
    recorrerFlores
  fin
  robot robot2
  comenzar
    recorrerPapeles
    Pos(76,1)
    recorrerPapeles
  fin
variables
  bot1:robot1
  bot2:robot2
comenzar
  AsignarArea(bot1,area1)
  AsignarArea(bot1,area3)
  AsignarArea(bot2,area4)
  AsignarArea(bot2,area2)
  Iniciar(bot1,1,1)
  Iniciar(bot2,26,1)
fin